#include <stdio.h>
#include<conio.h>
main() 
{
    float celsius, fahrenheit;
    clrscr();
    printf("Enter temperature in Celsius: ");
    scanf("%f", &celsius);
    fahrenheit = (9.0 / 5.0 * celsius) + 32;
    printf("Enter temperature in Celsius:%.2f\n", fahrenheit);
    getch();
    return 0;

}
